# mount point   fstype      device                   device2
/efs            ext4        /dev/block/mmcblk0p3                           flags=display="EFS";backup=1
/boot           emmc        /dev/block/mmcblk0p9                           flags=display="Kernel";backup=1
/recovery       emmc        /dev/block/mmcblk0p10
/modem          emmc        /dev/block/mmcblk0p13                           flags=display="Modem";backup=1
/cache          f2fs        /dev/block/mmcblk0p19
/system         ext4        /dev/block/mmcblk0p18
/preload        ext4        /dev/block/mmcblk0p20
/data           f2fs        /dev/block/mmcblk0p21    
/external_sd    vfat        /dev/block/mmcblk1p1     /dev/block/mmcblk1p1          flags=display="Micro SDcard";storage;wipeingui;removable
